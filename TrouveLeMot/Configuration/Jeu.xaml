<Window x:Class="Configuration.Jeu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Configuration"
        mc:Ignorable="d"
        Title="Jeu" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="txtbNbManches" HorizontalAlignment="Left" Height="70" Margin="631,24,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" TextChanged="TxtbNbManches_TextChanged"/>
        <TextBox x:Name="txtBmotCach" HorizontalAlignment="Left" Height="29" Margin="145,37,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="274" TextChanged="TxtBmotCach_TextChanged"/>
        <TextBox x:Name="txtBjoueur" HorizontalAlignment="Left" Height="23" Margin="145,264,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="274"/>
        <Button x:Name="btnTry" Content="Tenter" HorizontalAlignment="Left" Margin="145,310,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="btnNext" Content="Mot/manche suivante" HorizontalAlignment="Left" Margin="620,367,0,0" VerticalAlignment="Top" Width="131"/>
    </Grid>
</Window>
